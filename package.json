{
  "name": "ai-agent-arena",
  "version": "0.1.0",
  "private": true,
  "description": "Compare AI agent frameworks for a travel planning system",
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format-staged": "lint-staged",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcusElwin/ai-agent-arena.git"
  },
  "keywords": [
    "ai",
    "agents",
    "travel",
    "mastra",
    "openai",
    "pydantic"
  ],
  "author": "Marcus Elwin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarcusElwin/ai-agent-arena/issues"
  },
  "homepage": "https://github.com/MarcusElwin/ai-agent-arena#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "semantic-release": "^23.0.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
